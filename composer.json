{
    "name": "liuggio/rackspace-cloud-files-bundle",
    "type": "symfony-bundle",
    "description": "Provides rackspace cloud files support for #Symfony2 Bundles",
    "keywords": ["cloud", "rackspace", "assetic"],
    "license": "MIT",
    "authors": [
        {
            "name": "liuggio",
            "homepage": "http://www.welcometothebundle.com/"
        },
        {
            "name": "tvision contributions",
            "homepage": "https://github.com/tvision"
        }
    ],
    "autoload": {
        "psr-0": { "Liuggio\\RackspaceCloudFilesBundle": "" }
    },
    "target-dir": "Liuggio/RackspaceCloudFilesBundle",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rackspace/php-cloudfiles",
                "version": "master",
                "source": {
                    "url": "git://github.com/rackspace/php-cloudfiles.git",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/rackspace/php-cloudfiles/zipball/master",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.*",
        "rackspace/php-cloudfiles": "master",
        "liuggio/rackspace-cloud-files-streamwrapper": "master"
    }

}
